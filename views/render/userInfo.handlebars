<div id="userInfo">
    <h1 id="userID">{{data._id}}</h1>
    <h1>
        {{data.firstName}} {{data.lastName}}
    </h1>

    <div id="userInfoCat">
        <a href="" id="userBioClick">User Information</a>
        <a href="" id="userPostsClick">User Posts</a>
        <a href="" id="userLikedClick">Liked by User</a>
        <a href="/editprofile" id="editProfileClick">Edit profile</a>
    </div>

    <div id="userBio">
        <label>Age: {{data.age}}</label>
        <br>
        <label>Gender: {{data.gender}}</label>
        <br>
        <label>From: {{data.city}}, {{data.state}}</label>
    </div>

    <div id="userPosts">

    </div>

    <footer>
        <a href="/feed">Feed</a>
        <a href="/logout">Logout</a>
    </footer>
</div>