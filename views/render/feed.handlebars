<div>
    <h1>
        This is going to be the feed
    </h1>
    <div class="newPost">
        <form action="" class="newPost" id="newPostForm">
            <div id="titleDiv">
                <label for="title">Title</label>
                <input type="text" id="title" name="title">
            </div>
            <div id="catDiv">
                <label for="cat">Category</label>
                <input type="text" id="cat" name="cat">
            </div>
            <div id="ratingDiv">
                <label for="rating">Rating</label>
                <input type="text" id="rating" name="rating">
            </div>
            <div>
                <label for="review">Review</label>
                <textarea name="review" id="review" cols="50" rows="10"></textarea>
            </div>
            <button id="btn">Post</button>
        </form>
    </div>
    <div id="feed">

    </div>

    <ul id="reviewList"></ul>
    <div id="reviewContent" hidden></div>
    <div class="basicForm">
        <form id="likeForm" {{{showLike}}}>
            <div class="form-group" id="like">
                <input id="like_id" value="{{postId}}" hidden></input>
                <button type="submit" class="btn-"></button>
                <div id="num1">5</div> 
            </div>
        </form>
        <form id="dislikeForm" {{{showDislike}}}>
            <div class="form-group" id="dislike">
                <input id="dislike_id" value="{{postId}}" hidden></input>
                <button type="submit" class="btn-"></button>
                <div id="num2">5</div> 
            </div>
        </form>
        <form id="reviewComment" hidden>
            <div class="form-group">
                <label id="formLabel" for="reviewComment_input">Comment</label>
                <textarea class="form-control" id="reviewComment_input" rows="3" value="{{revComment}}"></textarea>
            </div>
            <div class="form-group">
                <button type="submit">Submit</button>
            </div>
    </form>
    </div>

    <script src="https://code.jquery.com/jquery-2.2.4.min.js" integrity="sha256-BbhdlvQf/xTY9gja0Dq3HiwQF8LaCRTXxZKRutelT44="
    crossorigin="anonymous"></script>
    <script src="/public/js/like.js"></script>

</div>